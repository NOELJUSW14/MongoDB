<div></div>
<h1 class="card center-align">Latest Sunday Funnies</h1>
<div class="row">
    <div class="col s3"></div>
    <h3 class"card center-align">Click "Add" to save</h3> 
<div class="row">
    <div class="col s2"></div>
    <div class="col s8 left-align">
        {{#each Comics}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <a href="/readComic/{{this.id}}" id="btnReadComic">Read Comic!</a>
        </div>
        {{/each}}
    </div>
    <h2>Add a Comic</h2>
<form class="create-form">
  <div class="form-group">
    <button type="submit">Add Comic</button>
  </div>
</form>
</div>